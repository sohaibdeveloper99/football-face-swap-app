# Face Swap API Configuration
# Copy this file to .env and fill in your actual API keys

# Server Configuration
PORT=5000
FRONTEND_URL=http://localhost:3000

# API Keys (Get these from the respective providers)
# Facemint API Key (https://facemint.io)
FACEMINT_API_KEY=your_facemint_api_key_here

# PiAPI API Key (https://piapi.ai)
PIAPI_API_KEY=your_piapi_api_key_here

# AKOOL API Key (https://akool.com)
AKOOL_API_KEY=your_akool_api_key_here

# Security Settings
NODE_ENV=development

